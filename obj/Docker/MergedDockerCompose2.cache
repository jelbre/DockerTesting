services:
  testgateway:
    build:
      context: G:\Jelle\Testing\DockerTesting
      dockerfile: testgateway/Dockerfile
    depends_on:
      testservice:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: testgateway
    links:
    - testservice
    ports:
    - published: 5001
      target: 80
    - published: 5000
      target: 80
  testservice:
    build:
      context: G:\Jelle\Testing\DockerTesting
      dockerfile: testservice/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    hostname: testservice
    image: testservice
    ports:
    - published: 5001
      target: 80
version: '3.4'